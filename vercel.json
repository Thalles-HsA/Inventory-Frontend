{
    "version": 2,
    "builds": [
      {
        "src": "frontend-react/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "frontend-react/build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "frontend-react/(.*)",
        "dest": "/index.html"
      }
    ]
  }